<%= render '/components/navbar' %>


<div class="container-form-create">
  <div class="photo-create-form">
  </div>

  <div class="content-form">

    <div class="form">
      <%= simple_form_for @puppy, method: :post do |f| %>
      <%= f.input :name %>
      <%= f.input :birthdate %>
      <%= f.input :breed %>
      <%= f.input :photo %>
      <!--<%#= f.input :photo_cache as: :hidden %>-->
      <%= f.button :submit, class: "btn btn-warning" %>
      <% end %>
    </div>
  </div>
</div>
<%= link_to 'Back', :back, class: "btn btn-warning" %>
